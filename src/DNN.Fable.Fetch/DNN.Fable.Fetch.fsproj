<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>DNN.Fable.Fetch</PackageId>
    <Version>1.0.0</Version>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license >MIT</license>
    <projectUrl>https://github.com/SCullman/DNN.Fable.Connector</projectUrl>
    <Authors>Stefan Cullmann</Authors>
    <Company>Stefan Cullmann</Company>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="DNN.Fable.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Thoth.Fetch" Version="1.0.0" />
  </ItemGroup>
  <!-- Add source files to "fable" folder in Nuget package -->
<ItemGroup>
    <Content Include="*.fsproj; **\*.fs" PackagePath="fable\" />
</ItemGroup>

</Project>
